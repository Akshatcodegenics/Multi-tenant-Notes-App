{
  "version": 2,
  "builds": [
    { "src": "index.js", "use": "@vercel/node" },
    { "src": "frontend/package.json", "use": "@vercel/next" }
  ],
  "routes": [
    { "src": "^/health$", "dest": "/index.js" },
    { "src": "^/api/(.*)$", "dest": "/index.js" },
    { "src": "/(.*)", "dest": "frontend/$1" }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
